<form action="/user/add/" style="margin-left: 40px;">
    <button type="submit" class="btn btn-primary btn-lg">Add New Users</button>
</form>
<table id="user_list" class="table table-striped table-hover">
    <h1 style="margin-left: 40px;">User List</h1>
    <%- include('../part/message') %>
    <tr>
        <th>User Name</th>
        <th>Email</th>
        <th>Password</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    <%
    users.map(user=>{%>
        <tr style="text-align: center;">
            <td style="width: 250px;"><%= user.username %></td>
            <td style="width: 200px;"><%= user.email %></td>
            <td style="width: 200px;"><%= user.password %></td>
            <td><a href="/" class="btn btn-warning">Edit</a></td>
            <td>
                <%- include('../part/deleteForm',{url:`/user/${user._id}?_method=DELETE`}) %>
            </td>
        </tr>
        <%})
    %>
  </table>